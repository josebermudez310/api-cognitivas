{
    "name": "Jupyter Codespace",
    "image": "mcr.microsoft.com/devcontainers/python:3.11",
    "postCreateCommand": "pip install notebook jupyterlab",
    "postStartCommand": "jupyter lab --ip=0.0.0.0 --no-browser --NotebookApp.token=''",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-toolsai.jupyter"
            ]
        }
    },
    "features": {},
    "forwardPorts": [
        8888
    ]
}
